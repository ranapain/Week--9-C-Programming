#include <stdio.h>
#include <stdlib.h>

void countDistinct(int arr[], int n, int k) {
    int freq[100000] = {0};  // assuming arr[i] within range
    int distinct = 0;
    for (int i = 0; i < k; i++) {
        if (freq[arr[i]] == 0) distinct++;
        freq[arr[i]]++;
    }
    printf("%d ", distinct);

    for (int i = k; i < n; i++) {
        if (freq[arr[i - k]] == 1) distinct--;
        freq[arr[i - k]]--;
        if (freq[arr[i]] == 0) distinct++;
        freq[arr[i]]++;
        printf("%d ", distinct);
    }
}
