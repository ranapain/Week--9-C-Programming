#include <stdio.h>
#include <stdlib.h>

void findDuplicates(int arr[], int n) {
    int freq[n];
    for (int i = 0; i < n; i++) freq[i] = 0;

    for (int i = 0; i < n; i++) {
        freq[arr[i]]++;
    }

    int found = 0;
    for (int i = 0; i < n; i++) {
        if (freq[i] > 1) {
            printf("%d ", i);
            found = 1;
        }
    }
    if (!found) printf("-1");
}
